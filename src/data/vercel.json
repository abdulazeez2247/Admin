{
  "version": 3,
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/live-matches", 
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ]
}